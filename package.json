    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-storybook": "^9.1.13",
    "gh-pages": "^6.3.0",
    "globals": "^15.14.0",
    "msw": "^2.7.0",
    "msw-storybook-addon": "^2.0.6",
    "storybook": "^9.1.13",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.18.2",
